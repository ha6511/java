		int[] math = {100,100,100,100,100};
		
		int[] sum = new int[5];
		double[] avg = new double[5];
		String[] hak = new String[5];
		int[] rank = {1,1,1,1,1};
		
		// 총점, 평균, 학점
		for (int i = 0; i < sum.length; i++) {
			sum[i] = kor[i] + eng[i] + math[i];
			avg[i] = int ( ( sum[i] / 3.0 ) * 10 ) / 10.0
		}
		
		
		// rank
		
