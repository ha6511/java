package com.ict.edu;

public class Ex12 {

	public static void main(String[] args) {
		// break label 과 continue label
		// break lable : 레이블이 지정한 블록을 탈출한다.
		// continue lable : 레이블이 지정한 블록 수행문을 포기하고 다음회차로 진행 
		// lable 지정방법 => 레이블이름 : 
		//					 뒤나 밑에는 반복문 만 올 수 있다.
		
		for (int i = 1; i < 11; i++) {
			System.out.print(i + "\t");
		}
		System.out.println();
		
	}
}
