package com.ict.edu;

public class Ex11 {
	
	public static void main(String[] args) {
		for (int i = 1; i < 4; i++) {
			for (int j = 1; j < 10; j++) {
				for (int k = 1; k < 4; k++) {
					System.out.print(k + " * " + j + " = " + j * k);
				}
				System.out.println();
			}
			System.out.println();
		}
	}
}
